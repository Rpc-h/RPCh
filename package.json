{
  "name": "rpch",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "configs/*",
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "format": "turbo run format",
