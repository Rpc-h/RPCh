{
  "name": "@rpch/e2e",
  "version": "0.1.0",
  "private": true,
  "license": "LGPL-3.0",
  "scripts": {
    "prebuild": "node -p \"'export default \\'' + require('./package.json').version + '\\';'\" > src/version.ts",
    "build": "yarn prebuild && tsc",
    "dev": "exit 0",
    "format": "prettier --check \"src/**/*.{ts,md}\"",
    "lint": "eslint \"src/**/*.ts*\"",
    "test": "exit 0",
    "start": "bash ./src/start.sh",
    "start:e2e": "npx jest --ci --runInBand --forceExit"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}