{
  "name": "rpch",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "configs/*",
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "eslint-config-rpch": "*",
    "jest": "^29.2.2",
    "prettier": "latest",
    "ts-jest": "^29.0.3",
    "typescript": "^4.5.2",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.15"
}