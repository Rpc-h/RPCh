{
  "name": "@rpch/monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "configs/*",
    "packages/*",
    "apps/*",
    "devkit/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "format": "turbo run format",
    "format:fix": "turbo run format -- --check=false --write=true",
    "lint": "turbo run lint",
    "test": "turbo run test --",
    "test:e2e": "turbo run start --filter @rpch/e2e",
    "publish-packages": "yarn build && yarn lint && yarn test && changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.2",
    "turbo": "^1.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.15"
}